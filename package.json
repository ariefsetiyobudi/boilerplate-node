{
  "name": "boilerplate-node",
  "version": "1.0.1",
  "scripts": {
    "start": "concurrently --kill-others \"npm run backend:development\" \"npm run frontend:development\"",
    "backend:build": "node app.js",
    "backend:development": "nodemon app.js",
    "frontend:build": "webpack --progress --config webpack.config.build.js",
    "frontend:development": "cross-env NODE_ENV=dev webpack serve --progress --config webpack.config.development.js",
    "lint": "eslint *.js",
    "test": "node -c *.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "nodemon": "^3.1.0",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@prismicio/client": "^7.3.1",
    "@prismicio/helpers": "^2.3.9",
    "auto-bind": "^5.0.1",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^9.1.3",
    "body-parser": "^1.20.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "cssnano": "^6.1.0",
    "dat.gui": "^0.7.9",
    "errorhandler": "^1.5.1",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "fontfaceobserver": "^2.3.0",
    "glslify-loader": "^2.0.0",
    "gsap": "^3.12.5",
    "include-media": "^2.0.0",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "mini-css-extract-plugin": "^2.8.1",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "nodelist-foreach-polyfill": "^1.2.0",
    "normalize-wheel": "^1.0.1",
    "ogl": "1.0.6",
    "postcss-loader": "^8.1.1",
    "prefix": "^1.0.0",
    "promise": "^8.3.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "staticify": "^5.0.0",
    "stats.js": "^0.17.0",
    "ua-parser-js": "^1.0.37",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.10.0"
  }
}
